<h1>Listing demoscaffolds</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @demoscaffolds.each do |demoscaffold| %>
  <tr>
    <td><%= demoscaffold.title %></td>
    <td><%= demoscaffold.body %></td>
    <td><%= link_to 'Show', demoscaffold %></td>
    <td><%= link_to 'Edit', edit_demoscaffold_path(demoscaffold) %></td>
    <td><%= link_to 'Destroy', demoscaffold, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

